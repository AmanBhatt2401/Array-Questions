#include<bits/stdc++.h>
using namespace std;

int main(){
    int n;
    cin>>n;
    
    int arr[n];
    for(int i=0;i<n;i++){
       cin>>arr[i];
    }

   int sumall=0;
    
   for(int i=0;i<n;i++){
    int  sum=0;
    {
        for(int j=i;j<n;j++){
            sum=sum+arr[j];
            cout<<sum<<endl;
        }
    }
       for(int i=0;i<n;i++){
       sumall=sumall+sum;
       cout<<sumall<<endl;
   } 
   }
  
    return 0;
}
